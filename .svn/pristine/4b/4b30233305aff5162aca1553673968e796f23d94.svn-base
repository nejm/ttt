        <script src="https://cdn.datatables.net/responsive/2.1.1/js/responsive.bootstrap.min.js" type="text/javascript"></script>

       

        <script src="/Dashboard/resources/js/worldLow.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/ammap.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/mapHelper.js" type="text/javascript"></script>
        
        <script src="/Dashboard/resources/js/notify.min.js" type="text/javascript"></script>
        <!-- out local ng stuff -->
        <script src="/Dashboard/resources/js/smart-area.min.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/ngstorage.js"></script>
        <script src="/Dashboard/resources/js/directives.js"></script>
        <script src="/Dashboard/resources/js/jspdf.min.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/jspdf.plugin.autotable.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/angular-datatables.min.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/angular-datatables.bootstrap.min.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/angular-datatables.scroller.min.js" type="text/javascript"></script>
        <script src="/Dashboard/resources/js/app.js"></script>
        <script src="/Dashboard/resources/js/jsplumb.js"></script>
        <script src="/Dashboard/resources/js/firstController.js"></script>

        <script src="/Dashboard/resources/js/bootstrap-select.js"></script>
        <script src="/Dashboard/resources/js/sweetalert.min.js"></script>
        
        <style>

            #sortable { list-style-type: none; margin: 0; padding: 0; }
            #sortable li { margin: 3px 3px 3px 0; padding: 1px; float: left; width: 49%; height: 360px;text-align: center;}
            .my_container{
                position: relative;
                width: 100%;
                height: 100%;
            }
        </style>
  

    <body class="nav-md" ng-controller="FirstExampleController" ng-init="initOrConsult('#{user}', '#{id}')">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col menu_fixed">
                    <div class="left_col scroll-view scrollable" >
                        <div class="navbar nav_title" style="border: 0;">
                            <a href="/home" class="site_title"><span>Dashboard</span></a>
                        </div>

                        <div class="clearfix"></div>

                        <!-- menu profile quick info -->
                        <div class="profile clearfix">
                            <div class="profile_pic">
                                <img src="/Dashboard/resources/images/img.png" alt="..." class="img-circle profile_img" />
                            </div>
                            <div class="profile_info">
                                <span>Welcome,</span>
                                <h2>#{user}</h2>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <!-- /menu profile quick info -->

                        <br />

                        <!-- sidebar menu -->
                        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu" style="overflow: auto">
                            <div class="menu_section" >

                                <ul class="nav side-menu">
                                    <li class="active" id="myStats"><a>Statistiques utilisateur</a>
                                        <ul class="nav child_menu" id="myStatsChild">
                                            <li ng-repeat="s in statistiques.my" class="active">
                                                <a ng-click="addStatToDashboard(s)">{{s.name}}</a></li>
                                        </ul>
                                    </li>
                                    <li class="active" ><a id="roleStats">Statistiques role</a>
                                        <ul class="nav child_menu" id="roleStatsChild">
                                            <li ng-repeat="s in statistiques.others track by $index" id="role{{$index}}" onclick='slide(this)'>
                                                <a>{{s.name}}</a>
                                                <ul class="nav child_menu" id="role{{$index}}Child">
                                                    <li ng-repeat="a in s.data track by $index" >
                                                        <a ng-click="addStatToDashboard(a)">
                                                            {{a.name}}
                                                        </a>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                    <li class="active" id="desc">
                                        <a ng-click="showMsg()">Description
                                            <i class="fa fa-plus pull-right"/>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>

                        <!-- /sidebar menu -->

                    </div>
                </div>

                <!-- top navigation -->
                <div class="top_nav">
                    <div class="nav_menu">
                        <nav>
                            <div class="nav toggle">
                                <div class="dropdown">
                                    <a id="menu_toggle"  class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown">
                                        <i class="fa fa-bars"></i>
                                    </a>

                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="/Dashboard/home">
                                                Acceuil
                                                <i class="fa fa-home pull-right"></i>
                                            </a>

                                        </li>
                                        <li class="divider"></li>
                                        <li ng-show="fullUser.profile == 'A'" class="dropdown ng-hide">
                                            <a>Statistiques
                                                <i class="fa fa-angle-right pull-right"></i>
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li><a href="/Dashboard/new">Nouveau
                                                        <span class="fa fa-plus pull-right"></span>
                                                    </a>
                                                </li>
                                                <li><a href="/Dashboard/statistiques/visualize">Consulter
                                                        <span class="fa fa-eye pull-right"></span>
                                                    </a>
                                                </li>
                                                <li><a href="/Dashboard/statistiques/search">Modifier
                                                        <span class="fa fa-edit pull-right"></span>
                                                    </a>
                                                </li>

                                            </ul>
                                        </li>
                                        <li class="dropdown"><a>Dashboards
                                                <i class="fa fa-angle-right pull-right"></i>
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li><a href="/Dashboard/create/new">Nouveau
                                                        <span class="fa fa-plus pull-right"></span>
                                                    </a></li>
                                                <li><a href="/Dashboard/dashboards/consulte">Consulter
                                                        <span class="fa fa-eye pull-right"></span>
                                                    </a></li>
                                                <li><a href="/Dashboard/dashboards/search">Modifier
                                                        <span class="fa fa-edit pull-right"></span>
                                                    </a></li>
                                            </ul>
                                        </li>
                                        <li ng-show="fullUser.profile == 'A'" class="dropdown ng-hide">
                                            <a href="\Dashboard\users">Utilisateurs

                                            </a>
                                        </li>
                                        <li ng-show="fullUser.profile == 'A'">
                                            <a href="/Dashboard/roles">Rôles
                                            </a>

                                        </li>
                                    </ul>
                                </div>
                            </div>


                            <ul class="nav navbar-nav navbar-right">
                                <li style="cursor: pointer" >
                                    <a class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <img src="/Dashboard/resources/images/img.png" alt=""  />${user}
                                        <span class=" fa fa-angle-down"></span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                                        <li><a href="javascript:;"> Profile</a></li>
                                        <li><a href="javascript:formSubmit()"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                                    </ul>
                                </li>

                                <li style="cursor: pointer">
                                    <a class="user-profile" ng-click="saveDashboard()">

                                        <span class=" fa fa-save"></span>
                                        Enregistrer
                                    </a>

                                </li>   
                            </ul>
                        </nav>
                    </div>
                </div>
                <!-- /top navigation -->

                <!-- page content -->
                <div class="right_col" role="main" style="height: 100%">

                    <div class="">
                        <div class="page-title">
                            <div class="title_left">
                                <h3>Dashboard</h3>
                            </div>
                            <div class="title_right">
                                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search for..." />
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button">
                                                <i class="fa fa-search" />
                                            </button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="clearfix"></div>
                        <ul id="sortable">
                            <li style="cursor: move;position: relative" ng-repeat="stat in dashboard track by $index" id="{{stat.id}}">
                                <div class="panel panel-default">
                                    <div class="panel-heading" style="height: 50px;">
                                        {{stat.title}}
                                        <div class="pull-right">
                                            <button class="btn btn-sm btn-default"  ng-click="addTitle($index)">
                                                <i class="fa fa-edit"/>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="panel-body" style="height: 250px">
                                        <div ng-if="stat.type != 'text'"><img height="200px" src='/Dashboard/resources/images/chart_line.png'  /></div>
                                        <div ng-if="stat.type != 'text'">{{stat.description}}</div>
                                        <div ng-if="stat.type == 'text'">
                                            <h5>{{stat.description}}</h5></div>
                                    </div>
                                    <div class="panel-footer" style="height: 60px;">
                                        <div>
                                            <small ng-if="stat.type != 'text'">
                                                {{stat.creationDate| date}}
                                            </small>

                                            <button ng-click='preview(stat.id)' class='btn btn-primary pull-right'  ng-if="stat.type != 'text'"><i class='fa fa-eye'></i> </button>
                                            <button ng-click='deleteStatFromDashboard($index)' class='btn btn-danger pull-right'><i class='fa fa-remove'></i> </button>
                                        </div>
                                    </div>
                                </div>

                            </li>
                        </ul>

                    </div>
                </div>

                <!-- footer content -->
                <form action="/Dashboard/logout" method="post" id="logoutForm">
                    <input type="hidden" name="${_csrf.parameterName}"
                           value="${_csrf.token}" />
                </form>

            </div>
        </div>
        
        <script type="text/javascript">
            function formSubmit() {
                document.getElementById("logoutForm").submit();
            }
            $("#myStats").on("click", function () {
                $("#myStatsChild").slideToggle();
            });

            $("#roleStats").on("click", function (e) {
                $("#roleStatsChild").slideToggle();
            });

            var slide = function (tag) {
                var idTag = $(tag).attr("id");
                $('#' + idTag + "Child").slideToggle();
            }
            
            $('#sortable').sortable();

        </script>
    </body>
</html>
