<js-plumb-object state-object="state" ng-style="{ 'left':state.x, 'top':state.y }" class="state_window" ng-mouseenter="setActiveState(state)">
    <h3 ng-click="setActiveState(state)">{{state.name}}</h3>
    <div style="position:absolute; top:50px; left:0px;">
        <js-plumb-endpoint ng-repeat="endpoint in state.targets" anchor="LeftMiddle" uuid="{{endpoint.uuid}}" settings="targetEndpointStyle"></js-plumb-endpoint>
    </div>
</js-plumb-object>
